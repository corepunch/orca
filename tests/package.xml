    <SystemMessage Message="KeyDown" Key="q">return</SystemMessage>
    <SystemMessage Message="WindowClosed">return</SystemMessage>
    <SystemMessage Message="RequestReload">window:refresh()</SystemMessage>
  </MessageLibrary>
</Project>
